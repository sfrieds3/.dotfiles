{
  "env": {
    "body": [
      "(env\n ($1 $2))"
    ],
    "description": "Snippet for an env stanza",
    "prefix": "env"
  },
  "executable": {
    "body": [
      "(executable\n (name $1)\n (public_name $2)\n (libraries $3))"
    ],
    "description": "Snippet for an executable stanza",
    "prefix": "executable"
  },
  "executables": {
    "body": [
      "(executables\n (names $1)\n (public_names $2)\n (libraries $3))"
    ],
    "description": "Snippet for an executables stanza",
    "prefix": "executables"
  },
  "install": {
    "body": [
      "(install\n (section $1)\n (files ($2.exe as $3)))"
    ],
    "description": "Snippet for an install stanza",
    "prefix": "install"
  },
  "library": {
    "body": [
      "(library\n (name $1)\n (public_name $2)\n (libraries $3))"
    ],
    "description": "Snippet for a library stanza",
    "prefix": "library"
  },
  "rule": {
    "body": [
      "(rule\n (alias $1)\n (deps $2)\n (targets $3)\n (action $4))"
    ],
    "description": "Snippet for a rule stanza",
    "prefix": "rule"
  },
  "test": {
    "body": [
      "(test\n (name $1)\n (libraries $2))"
    ],
    "description": "Snippet for a test stanza",
    "prefix": "test"
  }
}
